    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.21.2",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.19.3",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "googleapis": "^100.0.0",
    "js-base64": "^3.7.2",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.2",
    "multer-cloud-storage": "^2.8.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "nuxt": "^2.15.8",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
